├── build                   # Compiled files.
├── src                     # Source code.
│   ├── components            # React components.
│   ├── containers            # React components that are used as wrappers for pages or views.
│   ├── css                   # CSS Source code, global and module files.
│   ├── prism_lib             # Source code for prism module - https://prismjs.com/.
│   ├── App.tsx               # Entry react component.
│   ├── helpers.ts            # Helper functions for application.
│   ├── index.tsx             # React dom injecting.
│   ├── index.html            # HTML template for webpack.
│   ├── navigation.json       # Navigation settings.
│   └── types.ts              # General types for application.
├── webpack                 # Webpack config files.
│   ├── webpack.common.js     # Included in all compiles.
│   ├── webpack.config.js     # Entry file.
│   ├── webpack.dev.js        # Dev env settings.
│   └── webpack.prod.js       # Production env settings.
├── .babelrc                # Babel loader for webpack config.
├── .eslintrc.json          # eslint config file.
├── .gitignore              # Gitginore.
├── filestructure           # File structure text data.
├── filestructure.png       # File structure image.
├── tsconfig.json           # TypeScript config files.
└── README.md

